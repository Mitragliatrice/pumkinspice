<p id="notice"><%= notice %></p>

<h1>Skill Tables</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Experience length</th>
      <th>User rating</th>
      <th>Admin rating</th>
      <th>User notes</th>
      <th>Admin notes</th>
      <th>Date rated</th>
      <th>Link</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skill_tables.each do |skill_table| %>
      <tr>
        <td><%= skill_table.title %></td>
        <td><%= skill_table.experience_length %></td>
        <td><%= skill_table.user_rating %></td>
        <td><%= skill_table.admin_rating %></td>
        <td><%= skill_table.user_notes %></td>
        <td><%= skill_table.admin_notes %></td>
        <td><%= skill_table.date_rated %></td>
        <td><%= skill_table.link %></td>
        <td><%= link_to 'Show', skill_table %></td>
        <td><%= link_to 'Edit', edit_skill_table_path(skill_table) %></td>
        <td><%= link_to 'Destroy', skill_table, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Skill Table', new_skill_table_path %>
