<p id="notice"><%= notice %></p>

<h1>Skill Lists</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Experience length</th>
      <th>User rating</th>
      <th>Admin rating</th>
      <th>User notes</th>
      <th>Admin notes</th>
      <th>Date rated</th>
      <th>Link</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skill_lists.each do |skill_list| %>
      <tr>
        <td><%= skill_list.title %></td>
        <td><%= skill_list.experience_length %></td>
        <td><%= skill_list.user_rating %></td>
        <td><%= skill_list.admin_rating %></td>
        <td><%= skill_list.user_notes %></td>
        <td><%= skill_list.admin_notes %></td>
        <td><%= skill_list.date_rated %></td>
        <td><%= skill_list.link %></td>
        <td><%= link_to 'Show', skill_list %></td>
        <td><%= link_to 'Edit', edit_skill_list_path(skill_list) %></td>
        <td><%= link_to 'Destroy', skill_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Skill List', new_skill_list_path %>
